<h1>Products#index</h1>
<table>
<thead>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Price</th>
        <th>Stock Quantity</th>
    </tr>
</thead>
<tbody>
<% @products.each do |p| %>
    <tr>
        <td><%= p.title %></td>
        <td><%= p.description %></td>
        <td><%= p.price %></td>
        <td><%= p.stock_quantity %></td>
    </tr>
<% end %>
</tbody>
</table>